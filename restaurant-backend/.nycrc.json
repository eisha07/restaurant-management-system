{
  "all": true,
  "include": [
    "routes/**/*.js",
    "middleware/**/*.js",
    "config/**/*.js"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "coverage/**"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": false,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 75
}
